{
  "manifest_version": 3,
  "name": "JIRA Summary Viewer",
  "version": "1.0",
  "description": "Fetches and displays JIRA issue summaries.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8080/serap-be/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
